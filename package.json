{
  "name": "workoutlog",
  "version": "1.0.0",
  "description": "A domain-specific language for tracking and analyzing workout data",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run copy-static",
    "copy-static": "cp -r public/* dist/",
    "dev": "ts-node src/server.ts",
    "test": "jest",
    "test:parser": "ts-node src/tests/parser.test.ts",
    "test:exercises": "jest src/tests/exerciseReference.test.ts",
    "start": "node dist/server.js"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts"
    ]
  },
  "keywords": [
    "workout",
    "fitness",
    "dsl",
    "parser"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/pegjs": "^0.10.6",
    "codemirror": "^5.65.18",
    "css-loader": "^7.1.2",
    "express": "^4.18.3",
    "pegjs": "^0.10.0",
    "style-loader": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.2"
  }
}
